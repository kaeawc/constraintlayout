        android:stateListAnimator="@null"
        android:text="dup"
        android:textColor="@color/cal_adv_text"
        android:textSize="10pt"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTag="button4"
        app:roundPercent="@dimen/calc_round" />

    <androidx.constraintlayout.utils.widget.MotionButton
        android:onClick="key"
        android:id="@+id/adv_sin"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background= "@color/adv_background"
        android:stateListAnimator="@null"
        android:text="sin"
        android:tag="sin-1"
        android:textSize="10pt"
        android:textColor="@color/cal_adv_text"
        app:layout_constraintTag="button4"

        app:roundPercent="@dimen/calc_round" />

    <androidx.constraintlayout.utils.widget.MotionButton
